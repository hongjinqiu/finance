	</field>

	<field id="CREATE_BY" extends="ROOT_FIELD">
		<fieldName>CREATE_BY</fieldName>
		<displayName>创建人</displayName>
		<fieldDataType>INT</fieldDataType>
		<fixReadOnly>true</fixReadOnly>
		<defaultValueExpr>return
			com.geeboo.commons.utils.SessionUtils.getSessionUserId();
		</defaultValueExpr>
		
		<relationDS>
		</relationDS>
	</field>

	<field id="CREATE_TIME" extends="ROOT_FIELD">
		<fieldName>CREATE_TIME</fieldName>
		<displayName>创建时间</displayName>
		<fieldDataType>LONGINT</fieldDataType>
		<fieldNumberType>TIME</fieldNumberType>
		<fixReadOnly>true</fixReadOnly>
		<defaultValueExpr>return
			com.geeboo.commons.support.DateUtils.getCurrentTime(com.geeboo.commons.support.DateUtils.SHOW_DATE_FORMAT);
		</defaultValueExpr>
	</field>

