<?xml version="1.0" encoding="UTF-8"?>
<form-template xmlns="http://www.papersns.com/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.papersns.com/template http://www.papersns.com/template ">
	<id>SysUser</id>
	<data-source-model-id>SysUser</data-source-model-id>
	<adapter name="ModelFormTemplateAdapter" />
	<description>用户表单</description>
	<scripts>console/summary.js</scripts>
	<view-template view="Console/FormSchema.html"/>
	<html>
		<![CDATA[
			<script type="text/javascript">console.log("hello world")</script>
			<div>before toolbar</div>
		]]>
	</html>
	<toolbar>
		<button text="列表页" mode="fn" handler="doSave" iconCls="test">
			<expression>test_expression</expression>
			<button-attribute name="code" value="C" />
			<button-attribute name="functionId" value="50010" />
		</button>
		<button text="新增" mode="fn" handler="test" iconCls="">
			<button-attribute name="code" value="D" />
			<button-attribute name="functionId" value="50010" />
		</button>

		<button text="复制" mode="url" handler="doExport">
			<button-attribute name="code" value="U" />
			<button-attribute name="functionId" value="10018" />
		</button>

		<button text="修改" mode="fn" handler="test" iconCls="">
		</button>
		<button text="保存" mode="fn" handler="test" iconCls="">
		</button>
		<button text="放弃" mode="fn" handler="test" iconCls="">
		</button>
		<button text="删除" mode="fn" handler="test" iconCls="">
		</button>
		<button text="作废" mode="fn" handler="test" iconCls="">
		</button>
		<button text="反作废" mode="fn" handler="test" iconCls="">
		</button>
		<button text="刷新" mode="fn" handler="test" iconCls="">
		</button>
		<button text="修改日志" mode="fn" handler="test" iconCls="">
		</button>
	</toolbar>
	<html>
		<![CDATA[
			<div>after toolbar</div>
		]]>
	</html>

	<html>
		<![CDATA[
			<div>before column-model</div>
		]]>
	</html>
	<column-model name="A">
		<auto-column name="code" />
		<auto-column name="name" />
		<html>in column pos0</html>
		<auto-column name="startDate" colSpan="3" />
		<auto-column name="endDate" colSpan="3">
			<editor>
				<editor_attribute name="aaa" value="bb" />
				<editor_attribute name="aaa" value="cccc" />
			</editor>
		</auto-column>
		<html>
			hello html
			<editor>
				<editor_attribute name="aaa" value="bb" />
				<editor_attribute name="aaa" value="cccc" />
			</editor>
		</html>
		<select-column auto="true" name="" displayField="" valueField="" selectorName="" selectionMode="single" queryFunc="">
			<listeners change="" selection="" unSelection="" />
		</select-column>
	</column-model>


	<column-model name="B" selectionMode="radio" rownumber="true" bsonOrderBy="_id">
		<auto-column name="ttt" />
		<virtual-column name="">
			
		</virtual-column>
		<auto-column name="eee dddd">
			<editor>
				<editor_attribute name="" value=""></editor_attribute>
				<editor_attribute name="" value=""></editor_attribute>
				<editor_attribute name="" value=""></editor_attribute>
			</editor>
		</auto-column>
		<auto-column name="" colSpan="3" />
	</column-model>

