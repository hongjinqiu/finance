<?xml version="1.0" encoding="UTF-8"?>
<list-template xmlns="http://www.papersns.com/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.papersns.com/template http://www.papersns.com/template ">
	<id>SysUserSelector</id>
	<selector-id></selector-id>
	<data-source-model-id>SysUser</data-source-model-id>

	<adapter name="ModelListTemplateAdapter" />
	<description>用户列表</description>
	<scripts></scripts>
	<view-template view="Console/SelectorSchema.html"/>

	<toolbar export="true">
		<button text="新增" mode="fn" handler="doSave" iconCls="test">
			<expression>test_expression</expression>
			<button-attribute name="code" value="C" />
			<button-attribute name="functionId" value="50010" />
		</button>

		<button text="删除" mode="url^" handler="/oa/selfapply/egress/getEgress.go?EMPEGRESS_ID={EMPEGRESS_ID}">
			<button-attribute name="code" value="D" />
			<button-attribute name="functionId" value="50010" />
		</button>

		<button text="导出" mode="url" handler="doExport">
			<button-attribute name="code" value="U" />
			<button-attribute name="functionId" value="10018" />
		</button>

		<button text="获取记录测试" mode="fn" handler="test" iconCls="">
		</button>
	</toolbar>

	<security functionId="10018"></security>

	<!-- <before-build-query>SysUser.beforeBuildQuery</before-build-query> <after-build-query>SysUser.afterBuildQuery</after-build-query> -->
	<after-query-data></after-query-data>

	<data-provider size="10">
		<collection>SysUser</collection><!-- mongoDB collection name -->
		<fix-bson-query>{"_id": {"$gte": 15}}</fix-bson-query><!-- mongoDB fix query, json format -->
	</data-provider>

	<!-- <column-model selectionMode="checkbox" rownumber="true" sqlOrderBy="create_time desc"> -->
	<column-model selectionMode="radio" selectionTitle="已选用户" selectionTemplate="{code},{name}" rownumber="true" bsonOrderBy="_id" dataSetId="A">
		<toolbar>
			<button text="新增" mode="fn" handler="doSave" iconCls="test"></button>
		</toolbar>
		<checkbox-column name="checkboxSelect" hideable="false">
			<!-- <expression>"31" == str(record.get("AUDIT_STATUS"))</expression> -->
			<expression>1 == 1</expression>
			<!-- python language -->
		</checkbox-column>
		<!-- <id-column name="EMPEGRESS_ID" text="编号" hideable="true" /> -->
		<id-column name="_id" text="编号" hideable="true" />

		<virtual-column name="FUN_C" text="操作" width="40">
			<buttons>
				<button text="编辑" iconCls="bj_btn" mode="fn" handler="doEdit">
					<expression>1 == 1</expression>
					<button-attribute name="code" value="U" />
				</button>
				<button text="查看" iconCls="ck_btn" mode="url^" handler="/component/schema.go?@name=DEMO_VIEW&amp;EMPEGRESS_ID={id}">
					<expression>1 == 1</expression>
				</button>
			</buttons>
		</virtual-column>

		<auto-column name="id" />
		<auto-column name="code" />
		<auto-column name="name" />
		<string-column name="nick" auto="true" />
	</column-model>

	<query-parameters dataSetId="A">
		<query-parameter name="nick" auto="true" text=""></query-parameter>
		<query-parameter name="dept_id" auto="true" text=""></query-parameter>
		<query-parameter name="type" auto="true" text=""></query-parameter>
		<query-parameter name="createTimeBegin" columnName="createTime" text="" restriction="ge" auto="true" />
		<query-parameter name="createTimeEnd" columnName="createTime" text="" restriction="le" auto="true" />
		<query-parameter name="dictTest" auto="true" text=""></query-parameter>
		<query-parameter name="treeTest" auto="true" text=""></query-parameter>
		<query-parameter name="treeUrlTest" auto="true" text=""></query-parameter>
		<query-parameter name="checkboxTest" auto="true" text=""></query-parameter>
		<query-parameter name="radioTest" auto="true" text=""></query-parameter>
		<query-parameter name="_id" auto="true" text=""></query-parameter>
		<query-parameter name="nestTest" text="">
			<parameter-attribute name="config" value="{}" />
			<parameter-attribute name="listeners" value="{}" />
		</query-parameter>
	</query-parameters>
</list-template>
